#cross sections
a = input()
print(a[0:4])

s = str(input())
print(s[-3:])

s = str(input())
print(s[1::2])

s1 = input() 
s2 = input() 
print(s1[0::2], s2[1::2])

s = str(input())
print(s[4::-1])

a,b=map(str, input().split())
c = b[:len(a)]
print(c)

a, b = input().split()
c = a[:len(b)]
s = c[1::2] == b[1::2]
print(s)
