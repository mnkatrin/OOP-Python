s = input()
print(s[0].upper()+s[1:].lower())

s = input()
print(s.count('-'))

s = input()
print(s.find('ra'))

s = input()
a = s.replace('---','-')
b = a.replace('--','-')
print(b)

a,b,c = map(str, input().split())
print(a.rjust(3,'0'))
print(b.rjust(3,'0'))
print(c.rjust(3,'0'))

s = input()
print(s.count(' ')+1)

s=input()
print(s.replace(' ', ';'))
